{
  "name": "domo-sync",
  "version": "0.1.0",
  "description": "domo-sync",
  "main": "src/index.js",
  "author": "jloiola",
  "license": "ISC",
  "private": true,
  "scripts": {
    "start": "node src/index.js ",
    "dev": "nodemon ./src/index.js",
    "test": "jest",
    "test:ci": "CI=true jest",
    "test:watch": "jest --watch --verbose",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:debug-watch": "node --inspect-brk node_modules/.bin/jest --runInBand --watch",
    "coverage": "jest --coverage",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "prettier": "prettier --write .",
    "docs:build": "jsdoc -r ./src -c jsdoc.json",
    "docs": "open ./docs/index.html",
    "syncDomo": "sam local invoke -e sam/events/sync-domo.json --env-vars sam/env/local.json"
  },
  "engines": {
    "node": "14.16.1"
  },
  "dependencies": {
    "axios": "^0.23.0",
    "dotenv": "^10.0.0",
    "envalid": "^7.1.0",
    "google-auth-library": "^7.10.1",
    "googleapis": "^39.2.0",
    "knex": "^0.95.11",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsdoc": "^31.6.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-unicorn": "^27.0.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.7",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "tidy-jsdoc": "^1.4.0"
  },
